<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>update.resolv.conf</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/netwatch</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>WorkingDirectory</key>
    <string>/usr/local/etc/dnsmasq.d</string>
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/resolvconf.log</string>
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/resolvconf.err.log</string>
  </dict>
</plist>
