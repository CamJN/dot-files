#!/usr/local/bin/bash
#/usr/local/bin/brew-unbottled
brew info --json=v1 --installed | ruby -r json -r active_support -r 'active_support/core_ext/hash' -W0 -e 'puts JSON.load(STDIN.read).reject{|e|e["installed"].all? {|x| x["poured_from_bottle"]} }.map{|e| [e["versions"]["bottle"], e["name"] + " " + e["installed"][0]["used_options"].join(" ")] }.select{|e|e.shift}' | column -t
